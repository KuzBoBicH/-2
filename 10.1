using System;

class Program
{
    static void Main()
    {
        int n = 10; // кількість елементів масиву
        int[] arr = new int[n]; // створення масиву з n елементів

        Random rand = new Random(); // ініціалізація генератора випадкових чисел

        // заповнення масиву випадковими числами в діапазоні від -10 до 10
        for (int i = 0; i < n; i++)
        {
            arr[i] = rand.Next(-10, 11);
        }

        Console.Write("Масив: ");

        // вивід масиву на екран
        for (int i = 0; i < n; i++)
        {
            Console.Write(arr[i] + " ");
        }

        int index = -1; // змінна для зберігання індексу передостаннього додатного елемента

        // пошук передостаннього додатного елемента масиву
        for (int i = n - 2; i >= 0; i--)
        {
            if (arr[i] > 0)
            {
                index = i; // зберігаємо індекс додатнього елемента
                break; // зупиняємо цикл після знаходження першого додатнього елемента з кінця масиву
            }
        }

        if (index != -1) // якщо знайдено передостанній додатній елемент
        {
            Console.WriteLine("\nНомер передостаннього додатного елемента масиву: {0}", index);
        }
        else // якщо передостанній додатній елемент не знайдено
        {
            Console.WriteLine("\nПередостанній додатній елемент масиву не знайдено");
        }

        Console.ReadKey();
    }
}
