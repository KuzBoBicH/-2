using System;

class Program {
    static void Main(string[] args) {
        // Створюємо масив з 10 випадкових чисел в діапазоні [-10, 10]
        int[] Z = new int[10];
        Random random = new Random();
        for (int i = 0; i < Z.Length; i++) {
            Z[i] = random.Next(-10, 11);
        }

        // Виводимо значення масиву на екран
        Console.WriteLine("Масив Z:");
        foreach (int num in Z) {
            Console.Write(num + " ");
        }
        Console.WriteLine();

        // Обчислюємо кількість елементів, для яких виконується нерівність |Zi| > 5
        int countAbsMoreThan5 = 0;
        foreach (int num in Z) {
            if (Math.Abs(num) > 5) {
                countAbsMoreThan5++;
            }
        }
        Console.WriteLine("Кількість елементів масиву, для яких виконується нерівність |Zi| > 5: " + countAbsMoreThan5);

        // Обчислюємо суму елементів, значення яких менше 9
        int sumLessThan9 = 0;
        foreach (int num in Z) {
            if (num < 9) {
                sumLessThan9 += num;
            }
        }
        Console.WriteLine("Сума елементів масиву, значення яких менше 9: " + sumLessThan9);
    }
}
